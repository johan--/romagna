<ul class="nav well">
  <li id="diagram-container">
    <h4>Diagrams</h4>
    {{view Ember.Select 
      content=diagramList
      optionValuePath = 'content.id'
      optionLabelPath = 'content.title'
      optionView=App.DiagramOptionView
      selection = currentDiagram
      size=5
      elementId = 'diagrams'
    }}
    <label for="csx-file">csx:</label>
    {{file-upload elementId="csx-file" action="parseCSX"}}
    {{#if needsSQL}}
      <label for="sql-file">sql:</label>
      {{file-upload elementId="sql-file" value=sql action="parseSQL"}}
    {{/if}}
  </li>
  <li>
  <h4>Options</h4>
    {{label-radio content=objectLabelDisplayOptions value=objectLabelDisplay name="objectLabel"}}
  </li>
  <li>
  <h4>Queue</h4>
  {{view Ember.Select 
      content=diagramQueue 
      optionValuePath = 'content.id'
      optionLabelPath = 'content.title'
      size=5
      multiple=true
  }}
  </li>
  <li>
    <h4>Current Concept</h4>
    {{#with focusedConcept}}
      <h5>{{title}}</h5>
      <h6>Attributes</h6>
      <ul class="attribute-list">
        {{#each allAttributes}}
        <li>{{value}}</li>
      {{/each}}
      </ul>
      <h6>Objects</h6>
      <ul class="object-list">
        {{#each allObjects}}
          <li>{{value}}</li>
        {{/each}}
      </ul>
    {{/with}}
  </li>
</ul>
